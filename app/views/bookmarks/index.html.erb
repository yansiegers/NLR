<h1 class="text-xl font-bold">Graphs</h1>

<% if @bookmarks.any? %>
  <table>
    <thead>
      <tr>
        <th>ID</th>
        <th>Name</th>
        <th>User ID</th>
        <th>Favorite</th>
        <th>Created at</th>
        <th>Updated at</th>
      </tr>
    </thead>
    <tbody>
      <% @bookmarks.each do |graph| %>
        <tr>
          <td><%= graph.id %></td>
          <td><%= graph.name %></td>
          <td><%= graph.user_id %></td>
          <td><%= graph.favorite %></td>
          <td><%= graph.created_at %></td>
          <td><%= graph.updated_at %></td>
        </tr>
      <% end %>
    </tbody>
  </table>
<% end %>
